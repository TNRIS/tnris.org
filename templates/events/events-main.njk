{% extends "base.njk" %}

{% block topcontent %}
  <div class="container-fluid full">
    <div class="top-container">
      {% if mainimage %}
        <img class="img-responsive center-block" src="{{mainimage}}" alt="Masthead for {{title}}">
      {% endif %}

      <div class="top-header center-block">
        <div class="container">
          <h1>{{title}}</h1>
        </div>
      </div>
    </div>
  </div>
{% endblock %}

{% block contentsOne %}
{% endblock %}

{% block contentsTwo %}
  <div id="events-container" class="container">
    <div class="row">
      <div class="col-md-8">
        {{ contents|safe }}
      </div>
      <div class="col-md-3 meeting-box">
        <h3>
          <strong>July 23, 2019
            <i class="glyphicon glyphicon-star" style="color: #1e8dc1:"></i>
          </strong><br>3rd Quarter GIS Community Meeting
        </h3>

        <time>
          <i class="glyphicon glyphicon-time"></i>
          2PM-4PM</time><br>
        <location>
          <strong>Location:</strong><br>Texas Health and Human Services Commission, Brown-Heatly Building Room 1420,<br>4900 N. Lamar Blvd., Austin, Texas</location>
        <br><br>
        <a class="btn btn-tnris btn-md btn-block" href="https://tnris-org-static.s3.amazonaws.com/documents/GISComm_Agenda_20190723.pdf">
          <i class="fa fa-slideshare"></i>
          Download Agenda</a>
      </div>
    </div>
  </div>
{% endblock %}
