
{% extends "base.njk" %}
{% import "_form_macros.njk" as form %}

{% block styles %}
  {{ super() }}

  {{form.styles()}}
{% endblock %}

{% block scripts %}
  {{ super() }}

  {{form.scripts()}}

{% endblock %}

{% block topcontent %}
  <div class="full forum-2019-top">
    {% include "events/texas-gis-forum/2023/nav.njk" %}
    {% if mainimage %}
      <img src="{{mainimage}}" class="banner img-fluid mx-auto d-none d-md-block" alt="Texas GIS Forum Main Image">
    {% endif %}
    {% if mainimagesm %}
      <img src="{{mainimagesm}}" class="banner img-fluid mx-auto d-md-none d-lg-none" alt="Texas GIS Forum Main Image">
    {% endif %}
    {% include "events/texas-gis-forum/cta.njk" %}
  </div>
{% endblock %}


{% block contentsOne %}
  <div class="pt-4 container-md mx-auto">
      {{contents|safe}}
  </div>
{% endblock %}


{% block contentsTwo %}
<section class="forum-sponsors">
  {% include "events/texas-gis-forum/2023/sponsors.njk" %}
</section>
{% endblock %}