{% extends "base.njk" %}



{% block topcontent %}
<div class="container-fluid full">

      <div class="top-container">
      {% if mainimage %}
        <img class="img-responsive center-block" src="{{mainimage}}" alt="Masthead for {{title}}">
      {% endif %}

      <div class="top-header center-block">
          <div class="container">
            <h1>{{title}}</h1>
          </div>
      </div>
      </div>
</div>
{% endblock %}

{% block contentsOne %}
<section class="container">
 <div class="row">
    <ol class="breadcrumb">
      <li>
        <a href="/"><span class="glyphicon glyphicon-home"></span></a>
      </li>
      <li>
        <a href="/geographic-information-office">Geographic Information Office</a>
      </li>
      <li class="dcs-link"> <a href="/geographic-information-office/gis-solutions-group">Data Center Services (DCS) GIS Solutions Group</a> </li>
      <li class="active">{{title}}</li>
    </ol>
  </div>
  <div class="row article">
    <div class="col-sm-12 col-md-8 blog-main">
      <div class="gio-news-content">
        {{ contents|safe }}
      </div>
    </div>

    {% block rightContent %}
      <div class="col-sm-12 col-md-4 blog-sidebar">
          {% include "gio/gis-solutions-meeting.njk" %}

      </div>
    {% endblock %}
  </div>
</section>
{% endblock %}
