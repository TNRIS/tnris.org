<header class="navbar navbar-static-top bs-docs-nav" id="top" role="banner">
  <div class="title-header">
    <div class="container">
      <a href="/" class="navbar-brand logo-link" title="TNRIS Logo and back home">
        <span class="sr-only">Texas Natural Resources Information System</span>
      </a>
      <span class="tnris-name">Texas Natural Resources Information System</span>
      <a class="skip" href="#mainContentAnchor">Skip to Main Content</a>
      <a class="navbar-twdb pull-right hidden-xs" href="http://www.twdb.texas.gov/">A Division of the <span class="hidden-sm">Texas Water Development Board</span> <abbr class="hidden-md hidden-lg" title="Texas Water Development Board">TWDB</abbr></a>
    </div>
  </div>

  <div class="container nav-bg">
    <div class="navbar-header">
      <button class="navbar-toggle collapsed" type="button" data-toggle="collapse" data-target=".bs-navbar-collapse">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
    </div>
    <nav class="collapse navbar-collapse bs-navbar-collapse navbar-inverse" role="navigation">
      <ul class="nav navbar-nav">
        <li {% if id === 'maps-and-data' %}class="active"{% endif %}>
          <a class="data-hub-logo" href="https://data.tnris.org" title="Visit the New DataHub!">DataHub</a>
        </li>

        <li class="dropdown navbar-inverse">
          <a href="#" class="dropdown-toggle programs-nav " data-toggle="dropdown"><i class="fa fa-tasks hidden-xs hidden-sm"></i> Programs</a>
          <ul class="dropdown-menu inverse-dropdown" role="menu">
            {%- set programs = [
              {
                'id': 'stratmap',
                'url': '/stratmap',
                'name': 'StratMap - Strategic Mapping Program',
                'subpage_1': 'Stratmap Contracts',
                'subpage_1_url': '/stratmap/stratmap-contracts',
                'subpage_2': 'Orthoimagery',
                'subpage_2_url': '/stratmap/orthoimagery',
                'subpage_3': 'Elevation - Lidar',
                'subpage_3_url': '/stratmap/elevation-lidar',
                'subpage_4': 'Hydrography',
                'subpage_4_url': '/stratmap/hydrography',
                'subpage_5': 'Land Parcels',
                'subpage_5_url': '/stratmap/land-parcels',
                'subpage_6': 'Address Points',
                'subpage_6_url': '/stratmap/address-points'
              }, {
                'id': 'rdc',
                'url': '/research-distribution-center',
                'name': 'Research and Distribution Center (RDC)',
                'subpage_1': 'Historical Imagery Archive',
                'subpage_1_url': '/research-distribution-center/historical-imagery-archive',
                'subpage_2': 'Custom Maps',
                'subpage_2_url': '/research-distribution-center/custom-maps'
              }, {
                'id': 'training',
                'url': '/education',
                'name': 'Education & Training',
                'subpage_1': 'GIS Mentorships',
                'subpage_1_url': '/education/gis-mentorship',
                'subpage_2': 'Purchaser Resources',
                'subpage_2_url': '/education/purchasers'
              }, {
                'id': 'TexasImageryService',
                'url': '/texas-imagery-service',
                'name': 'Texas Imagery Service'
              }
            ] -%}
            {% for program in programs %}
              <li {% if id === program.id %}class="active"{% endif %}>
                <a href="{{program.url}}" class="main-program">
                  {{program.name}}<br>
                </a>
                {% if program.subpage_1 %}
                <ul>
                  <li><a href="{{program.subpage_1_url}}">{{ program.subpage_1 }}</a></li>

                {% if program.subpage_2 %}
                  <li><a href="{{program.subpage_2_url}}">{{ program.subpage_2 }}</a></li>
                {% endif %}

                {% if program.subpage_3 %}
                  <li><a href="{{program.subpage_3_url}}">{{ program.subpage_3 }}</a></li>
                {% endif %}

                {% if program.subpage_4 %}
                  <li><a href="{{program.subpage_4_url}}">{{ program.subpage_4 }}</a></li>
                {% endif %}

                {% if program.subpage_5 %}
                  <li><a href="{{program.subpage_5_url}}">{{ program.subpage_5 }}</a></li>
                {% endif %}

                {% if program.subpage_6 %}
                  <li><a href="{{program.subpage_6_url}}">{{ program.subpage_6 }}</a></li>
                {% endif %}

                </ul>
                {% endif %}
              </li>
            {% endfor %}
          </ul>
        </li>
        <li {% if id === 'geographic-information-office' %}class="active"{% endif %}>
          <a class="gio" href="/geographic-information-office">Geographic Information Office</a>
        </li>
      </ul>
      <ul class="nav navbar-nav navbar-right">
        <li {% if id === 'about' %}class="active"{% endif %}>
          <a href="/about">About<span class="sr-only"> us</span></a>
        </li>
        <li {% if id === 'contact' %}class="active"{% endif %}>
          <a href="/contact"><span class="glyphicon glyphicon-comment"></span> Contact Us</a>
        </li>
      </ul>
    </nav>
  </div>

  {% block topalert %}
  {% endblock %}
</header>
